&usbotg1 {
	vbus-supply = <&reg_usb_otg1_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg1>;
	disable-over-current;
	dr_mode = "otg";
	srp-disable;
	hnp-disable;
	adp-disable;
	status = "okay";
};

&usbotg2 {
	vbus-supply = <&reg_usb_otg2_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg2>;
	disable-over-current;
	dr_mode = "host";
	tpl-support;
	status = "okay";
};

&usbphy1 {
	fsl,tx-d-cal = <106>;
};

&usbphy2 {
	fsl,tx-d-cal = <106>;
};

&usdhc2 { /* eMMC (on module) */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	non-removable;
	bus-width = <8>;
	no-1-8-v;
	keep-power-in-suspend;
	status = "okay";
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_pdk2>;

	pinctrl_hog_pdk2: hog-pdk2-grp {
		fsl,pins = <
			MX6UL_PAD_UART1_CTS_B__GPIO1_IO18	0x400120b0 /* GPIO I -> BT_REG_ON (BT on LGA) */
			/*MX6UL_PAD_CSI_HSYNC__GPIO4_IO20		0x400120b0*/ /* GPIO J, uart 6 use! */
			/*MX6UL_PAD_CSI_PIXCLK__GPIO4_IO18	0x400120b0*/ /* GPIO K, disable while used for uart6 */
			/*MX6UL_PAD_CSI_MCLK__GPIO4_IO17		0x400120b0*/ /* GPIO L, disable while used for uart6 */
			/*MX6UL_PAD_CSI_VSYNC__GPIO4_IO19		0x400120b0*/ /* GPIO M, disable while used for uart6 */
