&ecspi2 {
	//compatible = "spi-gpio";
	cs-gpios = <&gpio4 22 GPIO_ACTIVE_LOW>, <&gpio3 2 GPIO_ACTIVE_LOW>, <&gpio3 3 GPIO_ACTIVE_LOW>, <&gpio3 4 GPIO_ACTIVE_LOW>, 
		<&gpiointrel 13 GPIO_ACTIVE_LOW>, <&gpiointrel 12 GPIO_ACTIVE_LOW>, <&gpiointrel 11 GPIO_ACTIVE_LOW>, <&gpiointrel 10 GPIO_ACTIVE_LOW> ;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi2>;
	power-supply = <&reg_spi_bus>;
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;	

	spidev@0{
		compatible = "spidev";
		reg = <0>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@1{
		compatible = "spidev";
		reg = <1>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@2{
		compatible = "spidev";
		reg = <2>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@3{
		compatible = "spidev";
		reg = <3>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@4{
		compatible = "spidev";
		reg = <4>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@5{
		compatible = "spidev";
		reg = <5>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@6{
		compatible = "spidev";
		reg = <6>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };
	spidev@7{
		compatible = "spidev";
		reg = <7>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <4000000>;
    };

